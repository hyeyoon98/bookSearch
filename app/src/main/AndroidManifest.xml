        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Health">
        <activity android:name="com.example.starBooks.SplashActivity">

        </activity>
        <activity android:name="com.example.starBooks.MainActivity">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>



        </activity>
        <activity android:name="com.example.starBooks.LoginActivity">


        </activity>
        <activity android:name="com.example.starBooks.RegisterActivity">

        </activity>

        <activity android:name=".DetailBookListActivity"/>

    </application>

</manifest>